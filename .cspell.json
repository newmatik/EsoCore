  "ignoreWords": [
    "YYYYMM",
    "YYYYMMDD",
    "HHMMSS",
    "kHz",
    "MHz",
    "GHz",
    "THz",
    "mA",
    "uA",
    "nA",
    "pA",
    "Nm",
    "dBFS",
    "DEVICEID",
    "SEQ",
    "vals"
  ],
  "patterns": [
    {
      "name": "Markdown links",
      "pattern": "\\[.*?\\]\\(.*?\\)",
      "description": "Ignore spell check in markdown links"
    },
    {
      "name": "Code blocks",
      "pattern": "```[\\s\\S]*?```",
      "description": "Ignore spell check in code blocks"
    },
    {
      "name": "Inline code",
      "pattern": "`[^`]*`",
      "description": "Ignore spell check in inline code"
    },
    {
      "name": "URLs",
      "pattern": "https?://[^\\s]+",
      "description": "Ignore URLs"
    },
    {
      "name": "File paths",
      "pattern": "\\b[a-zA-Z]:[/\\\\][^\\s]*|\\b\\./[^\\s]*|\\b\\.\\.?/[^\\s]*|\\b/[^\\s]*",
      "description": "Ignore file paths"
    },
    {
      "name": "Hex values",
      "pattern": "\\b0x[0-9a-fA-F]+\\b|\\b[0-9a-fA-F]{8,}\\b",
      "description": "Ignore hexadecimal values"
    },
    {
      "name": "Version numbers",
      "pattern": "\\b\\d+\\.\\d+(\\.\\d+)?([\\-\\+][a-zA-Z0-9\\-\\._]+)?\\b",
      "description": "Ignore version numbers"
    },
    {
      "name": "UUIDs",
      "pattern": "\\b[0-9a-f]{8}-[0-9a-f]{4}-[1-5][0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}\\b",
      "description": "Ignore UUIDs"
    }
  ],
  "enabledLanguageIds": [
    "markdown",
    "text",
    "javascript",
    "typescript",
    "python",
    "c",
    "cpp",
    "json",
    "yaml"
  ],
  "maxNumberOfProblems": 1000,
  "maxDuplicateProblems": 5,
  "minWordLength": 3,
  "flagWords": [
    "hte",
    "teh",
    "recieve",
    "seperate",
    "occured",
    "occurence",
